{
    "name": "acesseseucondominio/laravel-logger",
    "description": "Laravel logger for PHP / Laravel",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "monolog/monolog": "^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0|~9.0|~10.0"
    },
    "autoload": {
        "psr-4": {
            "AcesseSeuCondominio\\Logger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcesseSeuCondominio\\Logger\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AcesseSeuCondominio\\Logger\\LoggerServiceProvider"
            ]
        }
    }
}
